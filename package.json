{
  "name": "conway",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:notyourbear/conway.git",
  "author": "notyourbear <pracenberg@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "test": "eslint src && eslint tests && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build",
    "prod:build": "rimraf lib dist && babel src -d lib && NODE_ENV=production webpack -p --progress",
    "prod:start": "NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete all",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "husky": "^0.13.2",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "common-tags": "^1.4.0",
    "compression": "^1.6.2",
    "css-loader": "^0.26.4",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2"
  }
}
